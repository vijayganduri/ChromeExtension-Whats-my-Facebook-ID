{
  "manifest_version": 2,

  "name": "Find Facebook Id",
  "description": "Know any facebook id of any user or your favorite fan page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": {                    // optional
      "19": "icons/icon19.png",           // optional
      "38": "icons/icon38.png"            // optional
    },
   "default_title": "Find any facebook id", "default_popup": "popup.html"
  },
  "permissions": ["contextMenus"]
}
