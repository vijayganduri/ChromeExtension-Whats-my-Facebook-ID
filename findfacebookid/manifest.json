{
  "manifest_version": 2,

  "name": "What's my Facebook ID",
  "description": "Know facebook id of any user or your favorite fan page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": {                    
      "16": "icons/icon16.png",           // optional
      "19": "icons/icon19.png",           // optional
      "48": "icons/icon48.png",            // optional
      "128": "icons/icon128.png"
    },
   "default_title": "What's my Facebook ID", "default_popup": "popup.html"
  },
    "icons" : {
      "16": "icons/icon16.png",           // optional
      "19": "icons/icon19.png",           // optional
      "48": "icons/icon48.png",            // optional
      "128": "icons/icon128.png"
  }
}
